class:
	all types functions:
		parseRequest - get out the headers -> dictionary
		sendContinue - sends the continue if needed
		resolveFilename - figures out what file is wanted
						figures out its content type
		openResponse - try to open the file
						add open file to dictionary
		
		sendRespHeaders
		sendResponse
		sendError
		
	customized function:
		process - calls parseRequest in the appropriate way
		quit	- kills the processor in the right way
		
types:
	basic
	fork
	thread
	prethread

	
